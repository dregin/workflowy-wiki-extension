{
  "name": "Workflowy Wiki Extension",
  "version": "0.1.0",
  "description": "An Extension to allow worklfowy items link to a wiki page",
  "options_page": "options.html",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "storage",
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": [ "js/options.js"
      ]
    }
  ]
}